/////////////////////////////////
// shape tools
///////////////////////////////


.br2
  border-radius .75rem


section
  border none !important


.parallax__cover .vertical-guides
  height 300vh

  section
    border none !important


.vertical-guides
  overflow visible

.vertical-guides figure figure
  &:before,
  &:after
    //- z-index 1
    background rgba(255,255,255, 0.08)

.horizontal-guides
  margin-top 1px
  section
    position relative
  section:after
    margin 0 !important
    position absolute !important
    // z-index 99
    background rgba(255,255,255, 0.0015)

  section:first-child:after
    display none
  section:before
    background rgba(255,255,255, 0.0015)
    position absolute !important
    bottom 0
    // z-index 99


