.bg-paper:after
  content ""
  width 100%
  position relative
  height 50px
  float left
  position absolute
  top 0
  margin 0 !important
  background-image -webkit-gradient(linear, 0 0, 300% 100%, color-stop(0.25, #fff), color-stop(0.25, #fff))
  background-image linear-gradient(135deg, #fff 25%, transparent 25%), linear-gradient(225deg, #fff 25%, transparent 25%)
  background-position 50%
  background-repeat repeat-x
  background-size 50px 100%

.bbg-a:after
  background-image -webkit-gradient(linear, 0 0, 300% 100%, color-stop(0.25, $accent), color-stop(0.25, $accent))
  background-image linear-gradient(135deg, $accent 25%, transparent 25%), linear-gradient(225deg, $accent 25%, transparent 25%)


